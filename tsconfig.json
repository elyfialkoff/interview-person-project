{
   "type": "module",
   "compilerOptions":{ 
      "target":"ES2020",
      "moduleResolution":"node",
      "module":"commonjs",
      "declaration":false,
      "noLib":false,
      "emitDecoratorMetadata":true,
      "experimentalDecorators":true,
      "sourceMap":true,
      "pretty":true,
      "allowUnreachableCode":true,
      "allowUnusedLabels":true,
      "noImplicitAny":false,
      "noImplicitReturns":false,
      "noImplicitUseStrict":false,
      "outDir":"dist/",
      "baseUrl":"src/",
      "listFiles":false,
      "noEmitHelpers":true,
      "esModuleInterop":true,
      "importHelpers": true
   },
   "include":[ 
      "src/**/*", 
      "data.ts"   
   ],
   "exclude":[ 
      "node_modules"
   ],
   "compileOnSave":false
}